body{
  width: 100%;
  height: 100%;
}
page{
  width: 100%;
  height: 100%;
  overflow-y: hidden;
}
.container{
  position: relative;
  width: 100%;
  height: 100%;
}
.header{
  position: fixed;
  top: 0;
  left: 0;
  z-index:2;  
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  background: #FFFFFF;
}
.header_date{
  display: flex;
  height: 100%;
  align-items: center;
  margin-left: 226rpx;
}
.header_date view{
  font-size: 30rpx;
  color: #333;
}
.header_date image{
  margin-left: 16rpx;
  width: 36rpx;
  height: 36rpx;
}
.myCourse{
  width: 644rpx;
  height: 57rpx;
  position: fixed;
  left:0;
  top:100rpx;
  z-index:2;
  background: #fff;
  padding-left: 74rpx;
  padding-right: 32rpx;
  box-shadow: 0 -1rpx 0 0 #DDDDDD;
}
.course-header{
  margin: 24rpx 0 3rpx;
  width: 644rpx;
  height: 30rpx;
  display: flex;
  flex: 1;
  align-items: center;
}
.week-time text{
  display: block;
  height: 100%;
  width: 44rpx;
  margin-left: 56rpx;
  font-size: 22rpx;
  color: #9B9B9B;
}
.week-time:first-child text{
  margin-left: 0;
}
.course-main{
  width: 100%;
  display: flex;
  margin-top: 160rpx;
  padding-bottom: 100rpx;
}
.schedule{
  width: 50rpx;
  color: #9B9B9B;
  font-size: 18rpx;
}
.schedule view{
  width: 38rpx;
  height: 128rpx;
  padding: 0 6rpx;
}
.schedule view.schedule_changed{
  padding: 0 6rpx 0 1rpx;
}
.schedule view:last-child{
  margin-top: -14rpx;
  height: 64rpx;
}
.course-table{
  width: 700rpx;
  display: flex;
  position: relative;
  margin-top: 16rpx;
}
.content{
  width: 100rpx;
}
.content_column{
  width: 92rpx;
}
.content_chunk{
  width: 92rpx;
  height: 56rpx;
  margin-top: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 6rpx;
}
.content_chunk:nth-of-type(1){
  margin-top: 0;
}
.content_chunk.opened{
  background: #FFFFFF;
  box-shadow: inset 0 0 0 4rpx #F6F6F6;
}
.content_chunk.changeable{
  background: #EBEBEB;
  box-shadow: inset 0 0 0 4rpx #EBEBEB;
}
.content_chunk.immutable{
  box-shadow: inset 0 0 0 0 #EBEBEB;
  background: linear-gradient(45deg,
    #ebebeb , #ebebeb 0.30rem, #c7c7c7 0, #c7c7c7 0.35rem,
    #ebebeb 0,#ebebeb 0.55rem,#c7c7c7 0, #c7c7c7 0.60rem,
    #ebebeb 0,#ebebeb 0.80rem,#c7c7c7 0, #c7c7c7 0.85rem,
    #ebebeb 0, #ebebeb 1.05rem, #c7c7c7 0, #c7c7c7 1.10rem,
    #ebebeb 0,#ebebeb 1.30rem,#c7c7c7 0, #c7c7c7 1.35rem,
    #ebebeb 0,#ebebeb 1.55rem,#c7c7c7 0, #c7c7c7 1.60rem,
    #ebebeb 0,#ebebeb 1.80rem, #c7c7c7 0, #c7c7c7 1.85rem,
    #ebebeb 0,#ebebeb 2.05rem,#c7c7c7 0, #c7c7c7 2.10rem,
    #ebebeb 0,#ebebeb 2.30rem,#c7c7c7 0, #c7c7c7 2.35rem,
    #ebebeb 0,#ebebeb 2.55rem, #c7c7c7 0, #c7c7c7 2.60rem,
    #ebebeb 0,#ebebeb 2.80rem,#c7c7c7 0, #c7c7c7 2.85rem,
    #ebebeb 0,#ebebeb 3.05rem,#c7c7c7 0, #c7c7c7 3.10rem,
    #ebebeb 0,#ebebeb 3.30rem,#c7c7c7 0, #c7c7c7 3.35rem
  );
}
.content_chunk image{
  width: 36rpx;
  height: 36rpx;
}
.footer{
  position: fixed;
  left:0;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 32rpx;
  z-index: 2;
  background: #fff;
  color: #DDDDDD;
  box-shadow: 0 -1rpx 0 0 #DDDDDD;
  text-align: center;
}
.footer.actived{
  color: #FD5E02;
}
.pop-tip{
  position: fixed;
  left:0;
  bottom: 98rpx;
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 24rpx;
  transform: translate3d(0,100%,0);
  z-index: 1;
  transition:all 0.4s ease;
  color: #FFF;
  background: #FD5E02;
}
.pop-tip-top{
  transform: translate3d(0,0,0);
}
.mask{
  position: fixed;
  left:0;
  top:0;
  background: rgba(0,0,0,0.68);
  width:100%;
  height:100%;
  z-index:9;
}
.datetime_picker{
  position: fixed;
  background: #fff;
  left:0;
  bottom: 0;
  transform: translate3d(0,100%,0);
  transition:all 0.4s ease;
  width: 100%;
  height: 520rpx;
  z-index: 99;
}
.datetime_picker_top{
  transform: translate3d(0,0,0);
}
.handle_picker{
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  background: #F7F7F7;
}
.handle_picker view:nth-of-type(1){
  margin-left: 40rpx;
  height: 100%;
  line-height: 88rpx;
  font-size: 36rpx;
  color: #9B9B9B;
  font-weight: normal;
}
.handle_picker view:nth-of-type(2){
  margin-left: 154rpx;
  font-size: 36rpx;
  color: #4A4A4A;
}
.handle_picker view:nth-of-type(3){
  margin-left: auto;
  margin-right: 38rpx;
  height: 100%;
  line-height: 88rpx;
  font-size: 36rpx;
  color: #9B9B9B;
  font-weight: normal;
}
.pop-content{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 690rpx;
  transform: translate3d(0,100%,0);
  transition:all 0.4s ease;
  max-height: 832rpx;
  padding: 40rpx 30rpx 140rpx;
  background: #FFFFFF;
  box-shadow: inset 0 -1rpx 0 0 #DDDDDD;
}
.pop-content-top{
  transform: translate3d(0,0,0);
}
.content-header{
  font-size: 30rpx;
  color: #4A4A4A;
  font-weight: 500;
}
.pop-content .dateColor{
  color: #FD5E02;
}
.content-tip{
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #4A4A4A;
  font-weight: 500;
}
.pop-button{
  position: fixed;
  display: flex;
  left: 0;
  bottom: 0;
  width:100%;
  height: 98rpx;
}
.pop-button view{
  width: 50%;
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
}
.pop-button view:nth-of-type(1){
  font-weight: 400;
  font-size: 32rpx;
  color: #FD5E02;
  background: #FFFFFF;
  box-shadow: 0 -1px 0 0 #DDDDDD;
}
.pop-button view:nth-of-type(2){
  font-weight: 400;
  font-size: 32rpx;
  color: #FFFFFF;
  background: #FD5E02;
  box-shadow: 0 -1px 0 0 #DDDDDD;
}
.course-list{
  margin-top: 40rpx;
  width: 100%;
  max-height: 480rpx;
  overflow: auto;
}
.course-list .student_class{
  position: relative;
  margin-top: 36rpx;
  width: 100%;
}
.course-list .student_class:nth-of-type(1){
  margin-top: 0;
}
.course-list .leftBorder{
  position: absolute;
  left: 0;
  top: 0;
  width: 6rpx;
  height: 100%;
  background: #9B9B9B;
  border-radius: 6rpx;
}
.course-list .student_nick{
  margin-left: 20rpx;
  margin-bottom: 6rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #9B9B9B;
}
.course-list .student_course{
  margin-top: 4rpx;
  margin-left: 20rpx;
  font-size: 26rpx;
  font-weight: 300;
  color: #9B9B9B;
}
.course-list .student_course text{
  margin-left: 36rpx;
}
.course-list .student_course text:nth-of-type(1){
  margin-left: 0;
}
.successMask{
  position: fixed;
  left:0;
  top:0;
  background: rgba(0,0,0,0.01);
  width:100%;
  height:100%;
  z-index:99;
}
.successTip{
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate3d(-50%,-50%,0);
  z-index: 233;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 300rpx;
  height: 282rpx;
  background: rgba(0,0,0,0.68);
  border-radius: 20rpx;
}
.successTip image{
  margin-top: 40rpx;
  width: 144rpx;
  height: 144rpx;
}
.successTip text{
  margin-top: auto;
  margin-bottom: 40rpx;
  font-size: 28rpx;
  color: #FFFFFF;
}