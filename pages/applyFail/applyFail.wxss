.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
}

.container image{
  margin-top:282rpx;
  width: 228rpx;
  height: 228rpx;
}
.container text{
  display: block;
  text-align: center;
  width: 500rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #9B9B9B;
}
.container text:nth-of-type(1){
  margin-top: 50rpx;
}
.container view{
  margin-top: 120rpx;
  width: 370rpx;
  height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 80rpx;
  color: #9B9B9B;
  border: 2rpx solid #9B9B9B;
  border-radius: 6rpx;
}